        <SQL_FORM DB="MSSQL" RESTAB="Memory">
          <SQL_STMT_U val="SELECT&#xD;&#xA; NYS.ロットNO&#xD;&#xA;,NYS.商品コード&#xD;&#xA;,NYS.倉庫コード&#xD;&#xA;,MIN(NYS.有効期限)&#xD;&#xA;,SUM(NYS.現在庫数)&#xD;&#xA;&#xD;&#xA;FROM(&#xD;&#xA;SELECT&#xD;&#xA;  SHNYSTRN.ロットＮｏ ロットNO&#xD;&#xA;, SHNYSTRN.商品Ｃ 商品コード&#xD;&#xA;, SHNYSTRN.倉庫Ｃ 倉庫コード&#xD;&#xA;, SHNYSTRN.有効期限 有効期限&#xD;&#xA;&#xD;&#xA;/*入庫数*/&#xD;&#xA;, CASE WHEN SHNYSTRN.取引Ｄ区分 = 3 THEN SHNYSTRN.数量 ELSE 0 END 入庫数&#xD;&#xA;/*出庫数*/&#xD;&#xA;, CASE WHEN SHNYSTRN.取引Ｄ区分 &lt;> 3 THEN SHNYSTRN.数量 ELSE 0 END 出庫数&#xD;&#xA;/*数量*/&#xD;&#xA;, CASE WHEN SHNYSTRN.取引Ｄ区分 = 3 THEN SHNYSTRN.数量 ELSE SHNYSTRN.数量*(-1) END 現在庫数&#xD;&#xA;&#xD;&#xA;FROM 入出庫取引データ SHNYSTRN &#xD;&#xA;INNER JOIN 商品マスタ SHSHOMST &#xD;&#xA;ON (SHNYSTRN.取引部門=SHSHOMST.取引部門 AND SHNYSTRN.商品Ｃ=SHSHOMST.商品Ｃ)&#xD;&#xA;LEFT JOIN 区分マスタ KBN ON (KBN.区分Ｃ = 5&#x9;AND  KBN.区分 = SHNYSTRN.取引区分)&#xD;&#xA;&#xD;&#xA;WHERE&#xD;&#xA;/*検索条件*/&#xD;&#xA;KBN.預り入庫=0&#xD;&#xA; AND KBN.預り出庫=0&#xD;&#xA; AND SHSHOMST.ロット在庫区分=1&#xD;&#xA; AND SHSHOMST.在庫区分=0&#xD;&#xA; AND SHNYSTRN.ロット出荷Ｆ=0&#xD;&#xA; AND SHNYSTRN.商品Ｃ = ':1' /*商品CD*/&#xD;&#xA; AND SHNYSTRN.倉庫Ｃ = :2 /*倉庫CD*/&#xD;&#xA; AND SHNYSTRN.取引部門=:3&#xD;&#xA; AND KBN.在庫数対象 = 1&#xD;&#xA; &#xD;&#xA;) NYS&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;GROUP BY&#xD;&#xA;  NYS.ロットNO   /*ロット*/&#xD;&#xA;, NYS.商品コード /*商品*/&#xD;&#xA;, NYS.倉庫コード /*倉庫*/&#xD;&#xA;&#xD;&#xA;ORDER BY&#xD;&#xA;  NYS.商品コード /*商品*/&#xD;&#xA;, NYS.ロットNO   /*ロット*/&#xD;&#xA;, NYS.倉庫コード /*倉庫*/"/>
          <INARG>
