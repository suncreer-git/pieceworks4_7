          <SQL_FORM DB="MSSQL" RESTAB="MSSQL">
            <SQL_STMT_U val="SELECT&#xD;&#xA; JYU.商品Ｃ&#xD;&#xA;,sum(JYU.発注数)&#xD;&#xA;,sum(URI.仕入数)&#xD;&#xA;&#xD;&#xA;FROM (&#xD;&#xA;/*受注の読み込み*/&#xD;&#xA;&#xD;&#xA;SELECT   伝票Ｎｏ,&#xD;&#xA;         行Ｎｏ,&#xD;&#xA;&#x9; 商品Ｃ,&#xD;&#xA;         sum(数量) 発注数&#xD;&#xA;&#xD;&#xA;FROM     受発注データ&#xD;&#xA;      INNER JOIN&#xD;&#xA;         区分マスタ ON ( 区分Ｃ = 4 AND 区分 = 取引区分)&#xD;&#xA;&#xD;&#xA;WHERE    取引部門   = :1 AND&#xD;&#xA;         取引Ｄ区分 = 3  AND&#xD;&#xA;/*       伝票日付   BETWEEN '00000000' AND ':2' AND */&#xD;&#xA;         商品Ｃ     = ':3' AND&#xD;&#xA;         倉庫Ｃ     = :4   AND&#xD;&#xA;         入出荷Ｆ   = 0    AND&#xD;&#xA;         在庫数対象 = 1      /* 取引区分＝40 41 80 */&#xD;&#xA;&#xD;&#xA;GROUP BY 伝票Ｎｏ,&#xD;&#xA;         行Ｎｏ,&#xD;&#xA;&#x9; 商品Ｃ&#xD;&#xA;&#xD;&#xA;) JYU LEFT OUTER JOIN (&#xD;&#xA;&#xD;&#xA;/*受注計上された売上の読み込み*/&#xD;&#xA;&#xD;&#xA;SELECT   受注Ｎｏ,&#xD;&#xA;         受注行Ｎｏ,&#xD;&#xA;&#x9; 商品Ｃ,&#xD;&#xA;         sum( 数量 ) 仕入数&#xD;&#xA;&#xD;&#xA;FROM     取引データ&#xD;&#xA;      INNER JOIN&#xD;&#xA;         区分マスタ ON ( 区分Ｃ = 0 AND 区分 = 取引区分)&#xD;&#xA;&#xD;&#xA;WHERE    取引部門   = :1 AND&#xD;&#xA;         取引Ｄ区分 = 3  AND&#xD;&#xA;         受注Ｎｏ   &lt;>0  AND /* 発注№＝０は除外 */ &#xD;&#xA;         在庫数対象 = 1      /* 取引区分 = 40 41 80 */&#xD;&#xA;&#xD;&#xA;GROUP BY 受注Ｎｏ,&#xD;&#xA;         受注行Ｎｏ,&#xD;&#xA;&#x9; 商品Ｃ&#xD;&#xA;&#xD;&#xA;) URI ON (JYU.伝票Ｎｏ = URI.受注Ｎｏ&#xD;&#xA;      AND JYU.行Ｎｏ   = URI.受注行Ｎｏ&#xD;&#xA;      AND JYU.商品Ｃ   = URI.商品Ｃ)&#xD;&#xA;&#xD;&#xA;GROUP BY&#xD;&#xA; JYU.商品Ｃ"/>
